{
  "name": "opensans-webkit",
  "version": "1.0.0",
  "description": "Package for integrating Google Open Sans fonts in a web environment.",
  "scripts": {
    "compile": "npm run compile:development && npm run compile:production",
    "compile:development": "node-sass src/sass/open-sans.scss src/css/open-sans.css",
    "compile:production": "node-sass src/sass/open-sans.scss src/css/open-sans.min.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcourtet/opensans-webkit.git"
  },
  "keywords": [
    "open-sans",
    "google",
    "font",
    "font-face",
    "webfont"
  ],
  "author": "David Courtet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dcourtet/opensans-webkit/issues"
  },
  "homepage": "https://github.com/dcourtet/opensans-webkit#readme",
  "devDependencies": {
    "node-sass": "^4.9.0"
  }
}